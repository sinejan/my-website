{
	"name": "sinejan-portfolio",
	"version": "1.0.0",
	"description": "Modern TypeScript portfolio website for Şehra Sinejan Eser",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"start": "npm run dev",
		"type-check": "tsc --noEmit",
		"deploy": "npm run build && gh-pages -d dist",
		"predeploy": "npm run build",
		"build:gh": "npm run build && cp dist/index.html dist/404.html"
	},
	"keywords": [
		"portfolio",
		"typescript",
		"vite",
		"cloud-engineer",
		"devops"
	],
	"author": "Şehra Sinejan Eser <sehrasinejan@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.0",
		"gh-pages": "^6.1.0",
		"terser": "^5.43.1",
		"typescript": "^5.3.0",
		"vite": "^5.0.0"
	},
	"dependencies": {
		"lucide": "^0.294.0"
	}
}
